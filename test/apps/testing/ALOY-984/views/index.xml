<Alloy>
    <NavigationWindow platform="ios">
        <Window title="NavButtons">
            <LeftNavButtons>
                <View width="25" height="25" backgroundColor="red"/>
                <View width="25" height="25" backgroundColor="green"/>
                <View width="25" height="25" backgroundColor="blue"/>
            </LeftNavButtons>
            <RightNavButtons>
                <Button title="B1"/>
                <Button title="B2"/>
                <Button title="B3"/>
            </RightNavButtons>
            <Label text="The window should have three colored views on the left, and three buttons on the right, of the NavigationBar."/>
        </Window>
    </NavigationWindow>
	<Window platform="android,mobileweb">
		<Label text="This example is meant for iOS only"/>
	</Window>
</Alloy>
